            box-shadow: 0 8px 15px rgba(0, 0, 0, 0.1);
        }
        
        .career-path {
            position: relative;
            padding-left: 2rem;
            margin-bottom: 1.5rem;
        }
        
        .career-path::before {
            content: '';
            position: absolute;
            left: 0;
            top: 0.5rem;
            width: 1rem;
            height: 1rem;
            background: #667eea;
            border-radius: 50%;
        }
        
        .career-path::after {
            content: '';
            position: absolute;
            left: 0.5rem;
            top: 1.5rem;
            width: 2px;
            height: calc(100% - 0.5rem);
            background: #e5e7eb;
        }
        
        .career-path:last-child::after {
            display: none;
        }
        
